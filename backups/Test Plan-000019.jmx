        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="JSR223 Sampler">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import groovy.json.JsonOutput

StringBuilder ndjson = new StringBuilder()

for (int i = 1; i &lt;= 1000; i++) {
    // Dòng lệnh index
    ndjson.append(JsonOutput.toJson([index: [_index: &quot;products&quot;, _id: i]])).append(&quot;\n&quot;)
    
    // Dòng dữ liệu document
    def doc = [
        id       : i,
        name     : &quot;Product_$i&quot;,
        category : (i % 2 == 0) ? &quot;Electronics&quot; : &quot;Books&quot;,
        price    : 100 + (i % 50),
        timestamp: java.time.LocalDateTime.now().minusMinutes(i).toString()
    ]
    ndjson.append(JsonOutput.toJson(doc)).append(&quot;\n&quot;)
}

// ✅ Đảm bảo có đúng 1 dòng xuống dòng cuối cùng
ndjson.append(&quot;\n&quot;)

// Gán vào biến JMeter
vars.put(&quot;bulk_data&quot;, ndjson.toString())

// ✅ Ghi log kiểm tra NDJSON cuối
log.info(&quot;===== NDJSON (last 300 characters) =====&quot;)
log.info(ndjson.toString().takeRight(300))  // Chỉ lấy 300 ký tự cuối để dễ xem

// ✅ (Tùy chọn) Ghi tổng độ dài chuỗi
log.info(&quot;NDJSON total length: &quot; + ndjson.length())
</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
        </JSR223Sampler>
